<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="440.0" prefWidth="440.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bin.fxController.JoinServerController">
    <children>
        <TextField fx:id="addressTextField" alignment="CENTER" layoutX="125.0" layoutY="120.0" prefHeight="25.0" prefWidth="190.0" />
        <Label fx:id="addressIPLabel" alignment="CENTER" layoutX="125.0" layoutY="100.0" prefHeight="15.0" prefWidth="190.0" text="Address IP" textAlignment="CENTER" />
        <Label fx:id="portLabel" alignment="CENTER" layoutX="125.0" layoutY="170.0" prefHeight="15.0" prefWidth="190.0" text="Port" textAlignment="CENTER" />
        <TextField fx:id="portTextField" alignment="CENTER" layoutX="125.0" layoutY="190.0" prefHeight="25.0" prefWidth="190.0" />
        <Button fx:id="joinButton" layoutX="125.0" layoutY="250.0" mnemonicParsing="false" onAction="#joinButtonPress" onMouseEntered="#joinButtonSetOnMouseEntered" onMouseExited="#joinButtonSetOnMouseExited" prefHeight="40.0" prefWidth="190.0" text="Join">
         <font>
            <Font size="12.0" />
         </font></Button>
        <Button fx:id="backButton" layoutX="330.0" layoutY="401.0" mnemonicParsing="false" onAction="#backButtonPress" prefHeight="25.0" prefWidth="90.0" text="Back" />
    </children>
</AnchorPane>
